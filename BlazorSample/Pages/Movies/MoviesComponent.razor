@page "/movie"

<div>
    <h1>Movie Buff</h1>

    @if (lsMovies.Count <= 0)
    {
        <div>
            <p>Current no to display !</p>
            <button class="btn btn-outline-info"> Add Movie</button>
        </div>
    }
    else
    {
        @for (int i = 0; i < lsMovies.Count; i++)
        {
            <IndividualMovie currentMovie="lsMovies[i]" DeleteMovie="@DeleteMovie"></IndividualMovie>
        }
    }
</div>
@code {

    List<Movies> lsMovies = new List<Movies>();
    protected override void OnInitialized()
    {
        //InitialMovies();
    }

    public void InitialMovies()
    {
        Movies m1 = new Movies(1, "SpiderMan", DateTime.Now.ToString(),
        "data:image/jpeg;base64,");
        lsMovies.Add(m1);
        Movies m2 = new Movies(2, "IronMan", DateTime.Now.ToString(),
        "data:image/jpeg;base64,");
        lsMovies.Add(m2);
        Movies m3 = new Movies(3, "Hulk", DateTime.Now.ToString(), "data:image/jpeg;base64,");
        lsMovies.Add(m3);
        Movies m4 = new Movies(4, "AntMan", DateTime.Now.ToString(), "data:image/jpeg;base64,");
        lsMovies.Add(m4);
    }

    public async Task DeleteMovie(Movies currentMovie)
    {
        lsMovies.Remove(currentMovie);
    }
}
